{
  "name": "noem/http-module",
  "description": "description",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "biont",
      "email": "arbelzapf@gmail.com"
    }
  ],
  "require": {
    "nyholm/psr7": "^1.4",
    "nyholm/psr7-server": "^1.0",
    "middlewares/emitter": "^1.0",
    "middlewares/negotiation": "^2.0",
    "middlewares/whoops": "^2.0",
    "middlewares/request-handler": "^2.0",
    "middlewares/fast-route": "^2.0",
    "relay/relay": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.6",
    "noem/integration-test": "dev-master"
  },
  "extra": {
    "noem": {
      "factories": "factories.php",
      "extensions": "extensions.php"
    }
  },
  "autoload": {
    "psr-4": {
      "Noem\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Noem\\Http\\Tests\\": "tests/src"
    }
  },
  "scripts": {
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Biont/event-dispatcher"
    },
    {
      "type": "path",
      "url": "../integration-test"
    }
  ]
}
